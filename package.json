{
  "name": "smart-object-utils",
  "version": "1.0.0",
  "description": "⚡ Fast, lightweight JavaScript utility for deep cloning, nested object updates, and immutable data manipulation.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js"
    },
    "./src": {
      "import": "./src/index.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js --watch",
    "clean": "rimraf dist",
    "dev": "npm run build:watch",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:simple": "node tests/simple-test.js",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "npm run build && open dist/bundle-analysis.html",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\" \"examples/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\" \"examples/**/*.js\""
  },
  "keywords": [
    "deep-clone",
    "deep-clone-js",
    "fast-clone",
    "deep-merge",
    "nested-objects",
    "object-manipulation",
    "object-utils",
    "lodash-alternative",
    "cloneDeep",
    "merge-objects",
    "javascript",
    "javascript-utils",
    "utils",
    "immutable",
    "functional-programming",
    "performance",
    "tree-shakeable",
    "esm",
    "cjs",
    "umd",
    "typescript"
  ],
  "author": {
    "name": "Enes Erdoğan",
    "email": "enes@erdogan.co"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eneserdogan/smart-object-utils.git"
  },
  "bugs": {
    "url": "https://github.com/eneserdogan/smart-object-utils/issues"
  },
  "homepage": "https://github.com/eneserdogan/smart-object-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/preset-small-lib": "^11.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.esm.js",
      "limit": "15 KB",
      "name": "ES Modules build",
      "gzip": true
    },
    {
      "path": "dist/index.cjs.js",
      "limit": "16 KB",
      "name": "CommonJS build",
      "gzip": true
    },
    {
      "path": "dist/index.umd.js",
      "limit": "18 KB",
      "name": "UMD build",
      "gzip": true
    },
    {
      "path": "dist/index.min.js",
      "limit": "12 KB",
      "name": "Minified ES build",
      "gzip": true
    }
  ],
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "node": "16",
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not dead",
              "not ie 11"
            ]
          }
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "commonjs",
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "preset": "default",
    "testEnvironment": "node",
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/index.js"
    ],
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}